# Disable thermal throttling
settings put system setprop debug.thermal.throttle.support 0
settings put global setprop debug.thermal.throttle.support 0

# Disable thermal safety mode
settings put system setprop debug.thermal_safety_mode 0
settings put global setprop debug.thermal_safety_mode 0

# Disable thermal limit modes
settings put system setprop debug.thermal_limit_mode 0
settings put system setprop debug.thermal_limit_override 1
settings put global setprop debug.thermal_limit_mode 0
settings put global setprop debug.thermal_limit_override 1

# Maximize FPS (remove thermal restrictions)
settings put system setprop debug.fps.switch.thermal 0
settings put system setprop debug.thermal_fps_limit 999
settings put global setprop debug.fps.switch.thermal 0
settings put global setprop debug.thermal_fps_limit 999

# Disable display thermal dimming
settings put system setprop debug.hwc_thermal_dimming 0
settings put global setprop debug.hwc_thermal_dimming 0

# Set thermal limits to maximum
settings put system setprop debug.thermal_limit_refresh_rate 999
settings put global setprop debug.thermal_limit_refresh_rate 999

# Disable virtual thermal zones
settings put system setprop debug.virtual_thermal_thermal_zone 0
settings put global setprop debug.virtual_thermal_thermal_zone 0

# Force maximum performance mode
settings put system setprop debug.perf_mode 3
settings put system setprop debug.perf_topapp 1
settings put global setprop debug.perf_mode 3
settings put global setprop debug.perf_topapp 1

# Additional performance settings
settings put global setprop debug.window_animation_scale 0
settings put global setprop debug.transition_animation_scale 0
settings put global setprop debug.animator_duration_scale 0
settings put system setprop debug.peak_refresh_rate 144
settings put system setprop debug.min_refresh_rate 144
